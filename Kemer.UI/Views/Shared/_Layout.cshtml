    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="@ViewBag.Keyworld.description">
    @*<meta name="keywords" content="@ViewBag.Keyworld.keywords ">*@
    <meta name="author" content="@ViewBag.Keyworld.author">






    <link rel="icon" href="/images/imgs/@ViewBag.Keyworld.icon" accesskey="g">
    <title>@ViewBag.Keyworld.title</title>


    <!-- Font Awesome CSS -->
    <!-- Animate CSS -->
    <link rel="stylesheet" href="/css/animate.min.css" accesskey="g">

    <!-- Fancybox CSS -->
    <link rel="stylesheet" href="/css/jquery.fancybox.min.css" accesskey="g">

    <!-- OWL carousel CSS -->
