<!DOCTYPE html>
<html lang="en">
<head>

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="@ViewBag.Keyworld.description">
    @*<meta name="keywords" content="@ViewBag.Keyworld.keywords ">*@
    <meta name="author" content="@ViewBag.Keyworld.author">






    <link rel="icon" href="/images/imgs/@ViewBag.Keyworld.icon" accesskey="g">
    <title>@ViewBag.Keyworld.title</title>


    <!-- Font Awesome CSS -->
    <!-- Animate CSS -->
    <link rel="stylesheet" href="/css/style.min.css" accesskey="g">
    <link rel="stylesheet" href="/css/animate.min.css" accesskey="g">

    <!-- Fancybox CSS -->
    <link rel="stylesheet" href="/css/jquery.fancybox.min.css" accesskey="g">

    <!-- OWL carousel CSS -->
    <link rel="stylesheet" href="/css/owl.carousel.min.css" accesskey="g">
    <link rel="stylesheet" href="/css/owl.theme.default.min.css" accesskey="g">

    <!-- Main CSS -->
    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i&amp;subset=latin-ext" rel="stylesheet" accesskey="g">
