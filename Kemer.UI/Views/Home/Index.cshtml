                                    </div>

                                    <div data-height="10px" style="height: 10px;"></div>

                                    <a asp-action="Index" asp-controller="Product" class="btn btn-lg btn-round btn-primary mb-0" accesskey="g"><i class="fas fa-long-arrow-alt-right"></i>İncele</a>

                                    <div data-height="7vw" style="height: 7vw;"></div>

                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
        <!-- Benefits section -->
        <!-- Skills section -->
        <div class="section pt-5">

            <div class="container">

                <div class="row my-5">
                    <div class="col-md-10 offset-md-1 col-lg-8 offset-lg-2 col-xl-6 offset-xl-3">
                        <div class="text-center mt-4 mb-5">

                            <div class="pb-2 mb-4">
                                <img src="~/images/upload/kurumsallines.svg" data-width="120px" alt="image" />
                            </div>

                            <h2 class="h1 font-weight-800 mb-4">YETENEKLERİMİZ</h2>

