    <!-- Bootstrap -->







</head>
<body>

    <div class="navik-header header-shadow header-transparent navik-mega-menu viewport-sm" style="margin-top: 0px;">
        <div class="bg-indigo" style="width:100%">
            <div class="container">
                <div class="row align-items-center">

                    <div class="col-lg-6 d-none d-lg-block text-white-75 text-center text-lg-left py-3 py-lg-0">
                        <small>@ViewBag.TopHeader.SloganOne<a href="@ViewBag.TopHeader.Link" class="text-white font-weight-600" accesskey="g"> @ViewBag.TopHeader.SloganTwo </a></small>
                    </div>

                    <div class="col-lg-6 d-flex align-items-center justify-content-center justify-content-lg-end">
                        <a type="button" class="btn btn-indigo shadow-none rounded-0 px-3 mb-0" asp-action="Index" asp-controller="Login"><i class="fas fa-sign-in-alt"></i>Gİrİş Yap</a>
                        <a type="button" class="btn btn-indigo shadow-none rounded-0 px-3 mb-0" asp-action="Index" asp-controller="BayiForm"><i class="fas fa-pen-nib"></i>Bayİ Başvurusu</a>
                    </div>

                </div>
            </div>
        </div>
        <div class="container">
            <div class="navik-header-container">

                <!--Logo-->
                <div class="logo" data-mobile-logo="/images/upload/logo.svg" data-sticky-logo="/images/upload/logo.svg">
                    <a asp-action="Index" asp-controller="Home"><img src="/images/upload/logo.svg" alt="logo"></a>
                </div>

                <!-- Burger menu -->
                <div class="burger-menu">
                    <div class="line-menu line-half first-line"></div>
                    <div class="line-menu"></div>
                    <div class="line-menu line-half last-line"></div>
                </div>

                <!--Navigation menu-->
                <nav class="navik-menu menu-caret submenu-top-border submenu-scale">
                    <ul>
                        <li class="current-menu"><a asp-action="Index" asp-controller="Home">ANASAYFA</a></li>
                        <li>
                            <a href="#" accesskey="g">Kurumsal</a>
                            <ul>
                                <li><a asp-action="Index" asp-controller="About" accesskey="g">Hakkımızda</a></li>
                                <li><a asp-action="Index" asp-controller="Blog" accesskey="g">Blog</a></li>
                                <li><a asp-action="Index" asp-controller="Kurumsal" accesskey="g">Kurumsal Kimlik</a></li>


                            </ul>
                        </li>
                        <li class="mega-menu">
                            <a asp-action="Index" asp-controller="ServicePage" accesskey="g">HİZMETLER</a>
                        </li>
                        <li><a asp-action="Index" asp-controller="Product" accesskey="g">ÜRÜNLER</a></li>
                        <li class="mega-menu"><a asp-action="Index" asp-controller="Contact" accesskey="g">İLETİŞİM</a></li>

                    </ul>
                </nav>

            </div>
        </div>
        <div class="header-shadow-wrapper"></div>
    </div>


    <div class="bg-image" data-img-src="/images/imgs/@ViewBag.Slider.ImageUrl" style="background-image: url(/wwwroot/images/imgs/@ViewBag.Slider.ImageUrl);">
        <div class="container d-flex flex-column">
            <div class="row page-header-block-height">

                <div class="col-md-10 offset-md-1 col-lg-7 offset-lg-0 col-xl-5 d-flex py-lg-5 order-2 order-lg-1">
                    <div class="align-self-end w-100 pr-lg-0 pl-lg-4 ml-lg-n4 pb-5 mb-md-5 mb-lg-2 mb-xl-5 overflow-hidden text-center text-lg-left">
                        <div class="pb-2 mb-4 animated fadeInLeft" data-animated="fadeInLeft">
                            <img src="~/images/upload/kurumsallines.svg" data-width="120px" alt="resim" />
                        </div>
                        <h1 class="display-4 font-weight-800 mb-4 animated fadeInRight" data-animated="fadeInRight">@ViewBag.Slider.Header</h1>
                        <div class="lead-sm text-dark-gray pr-lg-5 pb-3 mb-5 animated fadeInLeft" data-animated="fadeInLeft">@ViewBag.Slider.Content</div>
                        <div class="animated fadeInRight" data-animated="fadeInRight">
                            @if (@ViewBag.Slider.ButonOneLink != null)
                            {
                                <a href="@ViewBag.Slider.ButonOneLink" class="btn btn-lg btn-round btn-primary mx-2 ml-lg-0" accesskey="g">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-credit-card" viewBox="0 0 16 16">
                                        <path d="M0 4a2 2 0 0 1 2-2h12a2 2 0 0 1 2 2v8a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V4zm2-1a1 1 0 0 0-1 1v1h14V4a1 1 0 0 0-1-1H2zm13 4H1v5a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1V7z" />
                                        <path d="M2 10a1 1 0 0 1 1-1h1a1 1 0 0 1 1 1v1a1 1 0 0 1-1 1H3a1 1 0 0 1-1-1v-1z" />
                                    </svg> &nbsp;@ViewBag.SLider.ButonOne
                                </a>
                            }

                            @if (@ViewBag.Slider.ButtonTwoLink != null)
                            {
                                <a href="@ViewBag.Slider.ButtonTwoLink" class="btn btn-lg btn-round btn-carrot mx-2" accesskey="g">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-stack" viewBox="0 0 16 16">
                                        <path d="m14.12 10.163 1.715.858c.22.11.22.424 0 .534L8.267 15.34a.598.598 0 0 1-.534 0L.165 11.555a.299.299 0 0 1 0-.534l1.716-.858 5.317 2.659c.505.252 1.1.252 1.604 0l5.317-2.66zM7.733.063a.598.598 0 0 1 .534 0l7.568 3.784a.3.3 0 0 1 0 .535L8.267 8.165a.598.598 0 0 1-.534 0L.165 4.382a.299.299 0 0 1 0-.535L7.733.063z" />
                                        <path d="m14.12 6.576 1.715.858c.22.11.22.424 0 .534l-7.568 3.784a.598.598 0 0 1-.534 0L.165 7.968a.299.299 0 0 1 0-.534l1.716-.858 5.317 2.659c.505.252 1.1.252 1.604 0l5.317-2.659z" />
                                    </svg> &nbsp;@ViewBag.SLider.ButtonTwo
                                </a>
                            }


                        </div>
                    </div>
                </div>

                <div class="col-lg-5 col-xl-5 offset-xl-2 d-flex order-1 order-lg-2 py-5 py-lg-0 my-4 my-lg-0">
                    <div class="align-self-center w-100 py-5 py-lg-0">
                        <a data-fancybox="" href="@ViewBag.Slider.VideoLink" class="play-btn play-btn-lg play-btn-auto-animate d-block m-auto" accesskey="g">

                            <svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" height="100px" width="100px" viewBox="0 0 100 100" enable-background="new 0 0 100 100" xml:space="preserve">
                            <path class="play-stroke" fill="#00acd1" stroke="none" d="M49.9,2.5C23.6,2.8,2.1,24.4,2.5,50.4C2.9,76.5,24.7,98,50.3,97.5c26.4-0.6,47.4-21.8,47.2-47.7C97.3,23.7,75.7,2.3,49.9,2.5"></path>
                            <path class="play-icon" fill="#fff" d="M39.855,36.68c0-2.2,1.559-3.1,3.464-2L66.39,48c1.905,1.1,1.905,2.9,0,4l-23.071,13.32c-1.905,1.1-3.464,0.2-3.464-2L39.855,36.68z"></path>
                            </svg>
                        </a>
                    </div>
                </div>

            </div>
        </div>
    </div>

    <div class="main-content pb-0">

        <div class="bg-image" data-img-src="/images/upload/section-bg-shape-03.png" srcset="/images/upload/section-bg-shape-03.png 190w">

            <!-- About Us section -->
            <div class="section pb-5 mb-5">
                <div class="container">
                    <div class="row align-items-center mt-md-4 mb-xl-5">

                        <div class="col-lg-6">
                            <img src="images/imgs/@ViewBag.WhoWeAre.ImageUrl" alt="image" class="img-fluid rounded-ultra shadow-lg mb-5 mb-lg-0 add-animate" data-animated="fadeInUp">
                        </div>

                        <div class="col-lg-6">
                            <div class="pl-lg-5">
                                <div class="pl-lg-4 add-animate" data-animated="fadeInRight">

                                    <div class="pb-2 mb-4">

                                        <img src="~/images/upload/kurumsallines.svg" data-width="120px" alt="image" />
                                    </div>

                                    <h2 class="h1 font-weight-800 mb-4"> @ViewBag.WhoWeAre.Header</h2>

                                    <div class="lead-sm mb-5">
                                        @ViewBag.WhoWeAre.Content
                                    </div>

                                    <div data-height="10px"></div>

                                    <a asp-action="Index" asp-controller="About" class="btn btn-lg btn-round btn-primary" accesskey="g">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-arrow-right" viewBox="0 0 16 16">
                                            <path fill-rule="evenodd" d="M1 8a.5.5 0 0 1 .5-.5h11.793l-3.147-3.146a.5.5 0 0 1 .708-.708l4 4a.5.5 0 0 1 0 .708l-4 4a.5.5 0 0 1-.708-.708L13.293 8.5H1.5A.5.5 0 0 1 1 8z" />
                                        </svg>&nbsp; İncele
                                    </a>

                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>

